	"minimum-stability": "stable",
	"prefer-stable": true,
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/brazorf/formula-parser"
		}
	],
    "require-dev": {
        "mockery/mockery": "^0.9.5"
    }

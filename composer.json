        }
    ],
    "require": {
        "denissimon/formula-parser": "dev-master",
		"mossadal/math-parser": "1.2.*"
    },
	"autoload": {
		"psr-4": {
			"Foothing\\Kpi\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Foothing\\Kpi\\Tests\\": "tests"
		}
